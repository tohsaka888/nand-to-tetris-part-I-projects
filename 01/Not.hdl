/**
 * Not gate:
 * out = not in
 * 思路： NOT a = NOT(a AND a) = NAND(a, a)
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=in, b=in, out=out);
}